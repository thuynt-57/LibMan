<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uet.k57ca.libview"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:logo="@drawable/logo"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name="uet.k57ca.libview.MainActivity"
            android:label=""
            android:logo="@drawable/logo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uet.k57ca.libview.SearchActivity"
            android:label=""
            android:logo="@drawable/logo"
            android:parentActivityName="uet.k57ca.libview.MainActivity" >
        </activity>
        <activity
            android:name="uet.k57ca.libview.BookDetailActivity"
            android:label=""
            android:logo="@drawable/logo"
            android:parentActivityName="uet.k57ca.libview.MainActivity" >
        </activity>
    </application>

</manifest>